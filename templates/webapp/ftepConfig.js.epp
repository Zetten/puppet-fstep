<%- |
    String $fstep_url,
    String $api_url,
    String $api_v2_url,
    String $sso_url,
    String $mapbox_token,
| -%>
// Managed by Puppet
define('fstepConfig', [], function () {
  return {
    fstepUrl: "<%= $fstep_url %>",
    apiUrl: "<%= $api_url %>",
    apiUrlv2: "<%= $api_v2_url %>",
    ssoUrl: "<%= $sso_url %>",
    mapboxToken: "<%= $mapbox_token %>"
  }
});
